boxplot(residuals(lm.HR1))
qqnorm(residuals(lm.HR1))
qqline(resid(lm.HR1))
plot(lm.HR3)
plot(fitted(lm.HR1), resid(lm.HR1))
binnedplot(fitted(lm.HR1), resid(lm.HR1))
