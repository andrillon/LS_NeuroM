lm.sw4 <- lmer(SWdens  ~ 1 + Elec*(BlockN + Treatment) + (1|SubID), data = SWS_data, REML = TRUE)
summary(lm.sw4)
confint(lm.sw4, method = "Wald", oldNames = FALSE)
